<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qiweili.healthapp">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Food.EditMeal" />
        <activity android:name=".profile.ProfileScreen" />
        <activity android:name=".Food.MyMeals" />
        <activity android:name=".Food.addFood" />
        <activity android:name=".Food.deleteMeal" />
        <activity android:name=".LoginScreen" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".friend.FriendScreen" />
        <activity android:name=".SignUpScreen" />
        <activity android:name=".Food.MyWater"></activity>
    </application>


</manifest>